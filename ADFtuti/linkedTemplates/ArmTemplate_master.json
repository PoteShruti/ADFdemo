{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFtuti"
		},
		"AzureDataLakeStorage1Linkedservice_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1Linkedservice'"
		},
		"SqlServer1_parametrized_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1_parametrized_ls'"
		},
		"SqlServer1linkedserviceDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1linkedserviceDB'"
		},
		"SqlServerLinkedServiceupsert_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerLinkedServiceupsert'"
		},
		"linkedserviceadftuti_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedserviceadftuti'"
		},
		"AzureDataLakeStorage1Linkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccounttuti.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFtuti_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linkedserviceadftuti_connectionString": {
						"value": "[parameters('linkedserviceadftuti_connectionString')]"
					},
					"AzureDataLakeStorage1Linkedservice_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1Linkedservice_accountKey')]"
					},
					"SqlServer1_parametrized_ls_connectionString": {
						"value": "[parameters('SqlServer1_parametrized_ls_connectionString')]"
					},
					"SqlServer1linkedserviceDB_connectionString": {
						"value": "[parameters('SqlServer1linkedserviceDB_connectionString')]"
					},
					"SqlServerLinkedServiceupsert_connectionString": {
						"value": "[parameters('SqlServerLinkedServiceupsert_connectionString')]"
					},
					"AzureDataLakeStorage1Linkedservice_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1Linkedservice_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADFtuti_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFtuti_ArmTemplate_0')]"
			]
		}
	]
}